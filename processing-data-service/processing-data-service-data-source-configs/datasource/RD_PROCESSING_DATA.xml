<datasource>
    <name>RD_PROCESSING_DATA</name>
    <description>PostgreSQL processing data</description>
    <jndiConfig useDataSourceFactory="false">
        <name>RDProcessingDataJNDI</name>
    </jndiConfig>
    <definition type="RDBMS">
        <configuration>
            <driverClassName>org.postgresql.Driver</driverClassName>
            <url>jdbc:postgresql://postgresql:5432/rd_processing_data</url>
            <username>postgres</username>
            <password>postgres</password>
        </configuration>
    </definition>
</datasource>
